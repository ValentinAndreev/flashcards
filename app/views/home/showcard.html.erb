<%= @cardcheck.inspect %>
<div class="text-center">
<h3>Слово: <%= @cardcheck.original_text %></h3><hr>
<%= form_tag(controller: "trainers", action: "review") do %>
  <%= label_tag(:text, "Перевод") %><br>
  <%= text_field_tag(:text) %>
  <%= hidden_field_tag "id", @cardcheck.id %>
  <%= submit_tag("Отправить") %>
<% end %>
</div>
<hr>
